{
  "name": "rugby-play-builder",
  "version": "1.0.0",
  "description": "Interactive rugby play visualization and animation tool",
  "homepage": "https://antzelino.github.io/rugby-play-builder",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_OPTIONS=\"--localstorage-file=/tmp/ls\" react-scripts start",
    "build": "cross-env NODE_OPTIONS=\"--localstorage-file=/tmp/ls\" react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "gh-pages": "^6.1.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "lucide-react": "^0.263.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
